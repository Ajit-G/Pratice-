pipeline{
  agent any
  stages{
    stage('git checkout'){
      steps{
        checkout(
        [$class: 'GitSCM', 
        branches: [[name: '*/master']], 
        extensions: [], 
        userRemoteConfigs: [[url: 'https://github.com/Ajit-G/Pratice-.git']]
        ])
      }
    }
    stage('git pull'){
      steps{
        sh 'git pull "https://github.com/Ajit-G/Pratice-/tree/testing-branch/newfolder"'
      }
    }
  }
}
