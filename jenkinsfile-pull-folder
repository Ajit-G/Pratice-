pipeline{
  agent any
  stages{
    stage('git checkout'){
      steps{
        checkout(
        [$class: 'GitSCM', 
        branches: [[name: '*/master']], 
        extensions: [], 
        userRemoteConfigs: [[url: 'https://github.com/Ajit-G/Pratice-.git']]
        ])
      }
    }
    stage('git pull'){
      steps{
        sh **
        mkdir new'
        cd new/
        git branch: 'testing-branch', url: 'https://github.com/Ajit-G/Pratice-.git'
        **
      }
    }
  }
}
